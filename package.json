{
  "name": "simpleplaywright",
  "version": "1.0.0",
  "description": "Basic Playwright automation project for ASX Online",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:chromium:headed": "playwright test --project=chromium --headed",
    "test:firefox:headed": "playwright test --project=firefox --headed",
    "test:webkit:headed": "playwright test --project=webkit --headed",
    "install-browsers": "playwright install"
  },
  "keywords": ["playwright", "automation", "testing"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  }
}
